    "esbuild": "^0.16.16",
    "google-spreadsheet": "^3.3.0",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "pluralize": "^8.0.0",
    "rimraf": "^3.0.2",
    "semistandard": "^16.0.1",
    "snazzy": "^9.0.0"
  },
  "dependencies": {}
